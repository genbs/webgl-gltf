{
	"compilerOptions": {
		"allowJs": false,
		"target": "ES6",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"outDir": "./dist",
		"baseUrl": "src",
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": false,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"sourceMap": true
	}
}
